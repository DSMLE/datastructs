value = ('sri',34)
key = ['name','age']
keyvalue= list(zip(key,value))
print(dict(keyvalue))
dict1= dict(keyvalue)
print(dict1)
#print(dict1,keyvalue)
dict1.update({'name':'SK'})
print(dict1)


for i in dict1.values():
    print(i)
for k,v in dict1.items():
    print('%s = %s' % (k,v))
    
    
    
